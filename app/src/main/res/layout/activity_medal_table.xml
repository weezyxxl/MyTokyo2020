    <!-- Cabecera -->
    <TableRow
        android:id="@+id/Cabecera"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#006597">

        <TextView
            android:id="@+id/ColumnaRank"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="2px"
            android:text="Rank"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/ColumnaCountry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="2px"
            android:text="Country"
            android:textColor="#ffffff"
            android:textSize="16sp"
            />

        <ImageView
            android:id="@+id/ColumnaOro"
            android:layout_gravity="center"
            android:padding="3px"
            android:text="Oro"
            android:src="@drawable/gold_medal"
            android:textColor="#ffffff"
            android:textSize="16sp"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"

            />

        <ImageView
            android:id="@+id/ColumnaPlata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="3px"
            android:text="Plata"
            android:textColor="#ffffff"
            android:src="@drawable/silver_medal"
            android:textSize="16sp"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"/>

        <ImageView
            android:id="@+id/ColumnaBronce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="3px"
            android:text="Bronce"
            android:textColor="#ffffff"
            android:textSize="16sp"
            android:src="@drawable/bronze_medal"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"/>

        <TextView
            android:id="@+id/ColumnaTotal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="2px"
            android:text="Total"
            android:textColor="#ffffff"
            android:textSize="16sp" />
    </TableRow>

